{
  "sections": [
    {
      "id": "genaral-details",
      "title": "GENERAL DETAILS",
      "idToShow": "FUND0001",
      "fields": [
        {
          "id": "fund-name",
          "label": "Fund Name",
          "placeholder": "Ex. Elliott Investment Management",
          "defaultValue": "Elliott Investment Management",
          "type": "text",
          "width": "50%",
          "required": true
        },
        {
          "id": "fund-type",
          "label": "Fund Type",
          "placeholder": "Select Fund Type",
          "defaultValue": "Hedge Fund",
          "type": "select",
          "options": [
            "Hedge Fund",
            "Private Equity Fund",
            "Private credit Fund",
            "REIT",
            "Pension Fund",
            "Mutual Fund"
          ],
          "width": "50%",
          "required": true
        },
        {
          "id": "fund-manager",
          "label": "Fund Manager",
          "placeholder": "Select Fund Manager",
          "defaultValue": "Growth Asset Management",
          "type": "select",
          "options": [
            "Capital X Asset Management Partners",
            "Growth Asset Management",
            "Silverline Capital Advisors",
            "Pinnacle Investment Group",
            "Innovative Equity Solutions",
            "Future Wealth Partners",
            "Blue Horizon Investments",
            "Vertex Capital Management",
            "Nexus Wealth Management",
            "Summit Financial Services"
          ],
          "width": "50%",
          "required": true
        },
        {
          "id": "base-currency",
          "label": "Base Currency",
          "placeholder": "Select Base Currency",
          "defaultValue": "USD - US Dollars",
          "type": "select",
          "options": ["USD - US Dollars", "INR - Indian Rupee"],
          "width": "50%",
          "required": true
        },
        {
          "id": "fund-admins",
          "type": "single-active-group",
          "addButtonLabel": "Add another fund admin?",
          "fields": [
            {
              "id": "fund-admin-1",
              "type": "text",
              "label": "Name of Fund Admin",
              "placeholder": "Ex. Harborview Fund Services",
              "defaultValue": "ClearLedger Solutions",
              "isActive": false
            },
            {
              "id": "fund-admin-2",
              "type": "text",
              "label": "Name of Fund Admin",
              "placeholder": "Ex. Harborview Fund Services",
              "defaultValue": "Harborview Fund Services",
              "isActive": true
            }
          ],
          "width": "50%",
          "required": true
        },
        {
          "id": "shadow-admins",
          "type": "single-active-group",
          "addButtonLabel": "Add another shadow?",
          "fields": [
            {
              "id": "shadow-admin-1",
              "type": "text",
              "label": "Name of Shadow (Optional)",
              "placeholder": "Ex. ClearLedger Solutions",
              "defaultValue": "ClearLedger Solutions",
              "isActive": true
            }
          ],
          "width": "50%"
        }
      ],
      "width": "100%"
    },
    {
      "id": "lifecycle",
      "title": "Lifecycle",
      "fields": [
        {
          "id": "stage",
          "label": "Stage",
          "placeholder": "Select Stage",
          "type": "select",
          "options": [
            "Receiving",
            "Deployment",
            "Monitoring",
            "Harvesting",
            "Wind-down"
          ],
          "defaultValue": "Deployment",
          "width": "50%",
          "required": true
        },
        {
          "id": "inception-date",
          "label": "Inception Date",
          "placeholder": "DD/MM/YYYY",
          "type": "date",
          "defaultValue": "15/08/2022",
          "width": "50%",
          "required": true
        },
        {
          "id": "investment-start-date",
          "label": "Investment Start Date",
          "placeholder": "DD/MM/YYYY",
          "type": "date",
          "defaultValue": "25/12/2024",
          "width": "50%",
          "required": true
        },
        {
          "id": "subscription",
          "label": "Commitment / Subscription",
          "type": "unit-frequency",
          "inputType": "number",
          "defaultValue": "10000000",
          "width": "50%",
          "placeholder": "Enter Amount",
          "unitLabel": "CCY"
        }
      ],
      "width": "100%"
    },
    {
      "id": "contact-details",
      "title": "CONTACT DETAILS",
      "fields": [
        {
          "id": "title",
          "label": "Title",
          "placeholder": "Ex. Mr.",
          "defaultValue": "Mr.",
          "type": "select",
          "options": ["Mr.", "Ms.", "Mrs."],
          "width": "12%",
          "required": true
        },
        {
          "id": "first-name",
          "label": "First Name",
          "placeholder": "Ex. John",
          "defaultValue": "John",
          "type": "text",
          "width": "38%",
          "required": true
        },
        {
          "id": "last-name",
          "label": "Last Name",
          "placeholder": "Ex. Doe",
          "defaultValue": "Doe",
          "type": "text",
          "width": "50%",
          "required": true
        },
        {
          "id": "email",
          "label": "Email ID",
          "placeholder": "Ex. johndoe@mail.com",
          "defaultValue": "johndoe@mail.com",
          "type": "text",
          "width": "50%",
          "required": true
        },
        {
          "id": "contact-number",
          "label": "Contact Number",
          "placeholder": "Ex. +1 (123) 456-7890",
          "defaultValue": "+1 (123) 456-7890",
          "type": "telephone",
          "width": "50%",
          "required": true
        }
      ],
      "width": "100%"
    },
    {
