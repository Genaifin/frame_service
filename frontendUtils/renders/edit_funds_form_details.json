          "options": [
            "Technology",
            "Healthcare",
            "Industrials",
            "Energy",
            "Consumer Goods"
          ],
          "width": "50%",
          "required": true
        },
        {
          "id": "geography",
          "label": "Geography",
          "placeholder": "Select Geography",
          "defaultValue": "USA",
          "type": "select",
          "options": [
            "USA",
            "Europe",
            "Asia",
            "Africa",
            "Australia",
            "Middle East",
            "Central America",
            "Southeast Asia"
          ],
          "width": "50%",
          "required": true
        },
        {
          "id": "strategy",
